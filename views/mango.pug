extends layout

block content
  h1= title
  - const jet = ['mango_type','mango_season','Cost']
  - let results;
  script.
    function buy(x){
      alert('Purchased Item: ' + x)
   } 
  div(class='container-fluid')
    div(class='row')
      each results, index in jet
        div(class='col-sm-2' onclick='buy('+index+')') #{results}